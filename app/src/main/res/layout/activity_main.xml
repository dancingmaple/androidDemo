<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    >

    <TextView
        android:id="@+id/tv_base_fragment"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="测试baseFragment"
        android:textSize="20sp"
        android:layout_marginBottom="5dp"
        android:background="@color/green"
        />

    <TextView
        android:id="@+id/tv_base_list_fragment"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="测试baseListFragment"
        android:textSize="20sp"
        android:layout_marginBottom="5dp"
        android:background="@color/green"
        />
    <TextView
        android:id="@+id/tv_base_viewpager_fragment"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="测试baseViewPagerFragment"
        android:textSize="20sp"
        android:layout_marginBottom="5dp"
        android:background="@color/green"
        />
    <TextView
        android:id="@+id/tv_base_head_list_fragment"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="测试baseHaveHeaderListFragment"
        android:textSize="20sp"
        android:layout_marginBottom="5dp"
        android:background="@color/green"
        />

</LinearLayout>